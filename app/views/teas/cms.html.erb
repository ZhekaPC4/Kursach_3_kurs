<h1>CMS чая</h1>
<ul>
  <% @teas.each do |tea| %>
    <div class="news">
      <a class="news_title" href="/tea/<%= tea.id %>">
        <%= tea.name %>
      </a>
      <div>
        <a class="tea_text"> Цена <%= tea.price%> </a>
        <a class="tea_text"> Вес <%= tea.weight%> </a>
      </div>
    </div>
  <% end %>
</ul>
<h3 class="pagination"> <%= paginate @teas %> </h3>